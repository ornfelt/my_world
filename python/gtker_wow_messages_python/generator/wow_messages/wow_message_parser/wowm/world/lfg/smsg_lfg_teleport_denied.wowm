enum LfgTeleportError : u32 {
    PLAYER_DEAD                = 1;
    FALLING                    = 2;
    IN_VEHICLE                 = 3;
    FATIGUE                    = 4;
    INVALID_LOCATION           = 6;
    /// azerothcore: It can be 7 or 8
    COMBAT                     = 8;
} {
    versions = "3.3.5";
}

smsg SMSG_LFG_TELEPORT_DENIED = 0x0200 {
    LfgTeleportError error;
} {
    versions = "3.3.5";
}
