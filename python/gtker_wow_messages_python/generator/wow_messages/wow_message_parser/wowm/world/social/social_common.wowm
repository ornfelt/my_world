flag GroupUpdateFlags : u32 {
    /// nothing
    NONE = 0x00000000;
    /// uint8, enum `GroupMemberOnlineStatus`
    STATUS = 0x00000001;
    /// uint16
    CUR_HP = 0x00000002;
    /// uint16
    MAX_HP = 0x00000004;
    /// uint8, enum Powers
    POWER_TYPE = 0x00000008;
    /// uint16
    CUR_POWER = 0x00000010;
    /// uint16
    MAX_POWER = 0x00000020;
    /// uint16
    LEVEL = 0x00000040;
    /// uint16
    ZONE = 0x00000080;
    /// uint16, uint16
    POSITION = 0x00000100;
    /// uint32 mask, for each bit set uint16 spellid
    AURAS = 0x00000200;
    /// uint16 above mask continuation, giving max total of 48 auras possible
    AURAS_2 = 0x00000400;
    /// uint64 pet guid
    PET_GUID = 0x00000800;
    /// pet name, NULL terminated string
    PET_NAME = 0x00001000;
    /// uint16, model id
    PET_MODEL_ID = 0x00002000;
    /// uint16 pet cur health
    PET_CUR_HP = 0x00004000;
    /// uint16 pet max health
    PET_MAX_HP = 0x00008000;
    /// uint8 pet power type
    PET_POWER_TYPE = 0x00010000;
    /// uint16 pet cur power
    PET_CUR_POWER = 0x00020000;
    /// uint16 pet max power
    PET_MAX_POWER = 0x00040000;
    /// uint32 mask, for each bit set uint16 spellid, pet auras...
    PET_AURAS = 0x00080000;
    /// uint16 above mask continuation, giving max total of 48 auras possible
    PET_AURAS_2 = 0x00100000;
    MODE_OFFLINE = 0x10000000;
} {
    versions = "1.12";
}

flag GroupMemberOnlineStatus : u8 {
    OFFLINE = 0x0000;
    /// `Lua_UnitIsConnected`
    ONLINE = 0x0001;
    /// `Lua_UnitIsPVP`
    PVP = 0x0002;
    /// `Lua_UnitIsDead`
    DEAD = 0x0004;
    /// `Lua_UnitIsGhost`
    GHOST = 0x0008;
    /// `Lua_UnitIsPVPFreeForAll`
    PVP_FFA = 0x0010;
    /// used in calls from `Lua_GetPlayerMapPosition`/`Lua_GetBattlefieldFlagPosition`
    ZONE_OUT = 0x0020;
    /// `Lua_UnitIsAFK`
    AFK = 0x0040;
    /// `Lua_UnitIsDND`
    DND = 0x0080;
} {
    versions = "1.12 2 3";
}

